{
  "yogi": {
    "location": "/Echoers/${HOSTNAME}",
    "connection": {
      "target": "127.0.0.1:10000",
      "timeout": null,
      "identification": null
    },
    "_server": {
      "address": "any",
      "port": 10001,
      "timeout": 3.0,
      "identification": null
    }
  },
  "logging": {
    "stdout": {
      "colourised": true,
      "max-verbosity": "INFO",
      "component-verbosity": {
        "App": "TRACE",
        "Yogi": "INFO"
      }
    },
    "yogi": {
      "max-verbosity": "INFO",
      "component-verbosity": {
        "App": "DEBUG",
        "Yogi": "INFO"
      }
    }
  }
}
