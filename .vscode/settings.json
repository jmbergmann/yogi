{
  "csharp.format.enable": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.vs": true,
    "**/.idea": true,
    "**/bin/": true,
    "**/obj/": true,
    "build*/": true,
    "**/.exe/": true,
    "**/__pycache__/": true,
    "**/*.dll": true,
    "**/node_modules/": true
  },
  "python.testing.cwd": "yogi-python",
  "python.testing.unittestArgs": ["-v", "-s", ".", "-p", "test_*.py"],
  "python.testing.unittestEnabled": true,
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": ["--max-line-length", "80"],
  "[python]": {
    "editor.rulers": [79]
  },
  "[c]": {
    "editor.rulers": [80]
  },
  "[cpp]": {
    "editor.rulers": [80]
  },
  "[csharp]": {
    "editor.rulers": [100]
  },
