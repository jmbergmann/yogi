{
  "env": {
    "definesFromBuildSystem": ["YOGI_TEST_DATA_DIR=\"/some/path\""]
  },
  "configurations": [
    {
      "name": "Mac",
      "includePath": [
        "${workspaceFolder}/yogi-core/include",
        "${workspaceFolder}/yogi-cpp/include",
        "${workspaceFolder}/vcpkg/installed/x64-osx/include",
        "/usr/local/include"
      ],
      "defines": ["${definesFromBuildSystem}"],
      "intelliSenseMode": "clang-x64",
      "macFrameworkPath": ["/System/Library/Frameworks"],
      "cppStandard": "c++14",
      "browse": {
        "path": ["${workspaceFolder}"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    },
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/yogi-core/include",
        "${workspaceFolder}/yogi-cpp/include",
        "${workspaceFolder}/vcpkg/installed/x64-linux/include",
        "/usr/include",
        "/usr/local/include"
      ],
      "defines": ["${definesFromBuildSystem}"],
      "intelliSenseMode": "clang-x64",
      "cppStandard": "c++14",
      "browse": {
        "path": ["${workspaceFolder}"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    },
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/yogi-core/include",
        "${workspaceFolder}/yogi-cpp/include",
        "${workspaceFolder}/vcpkg/installed/x64-windows-static/include"
      ],
      "defines": ["_DEBUG", "UNICODE", "WIN32"],
      "intelliSenseMode": "msvc-x64",
      "cppStandard": "c++14",
      "browse": {
        "path": ["${workspaceFolder}"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
