{
  "yogi": {
    "location": "/Account Manager",
    "connection": {
      "target": "127.0.0.1:10000",
      "timeout": null,
      "identification": null
    }
  },
  "logging": {
    "stdout": {
      "colourised": true,
      "max-verbosity": "INFO",
      "component-verbosity": {
        "App": "TRACE",
        "Yogi": "INFO",
        "Account": "TRACE",
        "AccountManager": "TRACE",
        "AccountProvider": "TRACE",
        "StorageProvider": "TRACE",
        "Session": "TRACE",
        "SessionManager": "TRACE"
      }
    },
    "yogi": {
      "max-verbosity": "INFO",
      "component-verbosity": {
        "App": "DEBUG",
        "Yogi": "INFO",
        "Account": "TRACE",
        "AccountManager": "TRACE",
        "AccountProvider": "TRACE",
        "StorageProvider": "TRACE",
        "Session": "TRACE",
        "SessionManager": "TRACE"
      }
    }
  },
  "account-provider": {
    "type": "file",
    "file": "/etc/yogi-accounts.json"
  },
  "account-storage-provider": {
    "type": "filesystem",
    "path": "/usr/share/yogi-account-manager/account-storage"
  },
  "session-storage-provider": {
    "type": "filesystem",
    "path": "/tmp/yogi-account-manager/session-storage"
  }
}
